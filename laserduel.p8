--laser duel
--by oc_ram

function _init()
	cls()
	ufos={}
	lasers={}
	setup_ufos()
	spawn_ufo(32,32)
	spawn_ufo(96,96)
end

function _update60()
--ufos and collisions
 foreach(ufos,update_ufo)
 for i=1,#ufos do
 	for j=i+1,#ufos do
 		collide_ufos(ufos[i], ufos[j])
 	end
 end
--lasers
	foreach(lasers,update_laser)
	foreach(lasers,update_laser_hit)
end

function _draw()
 cls(1)
 foreach(lasers,draw_laser)
 foreach(ufos,draw_ufo)
end

