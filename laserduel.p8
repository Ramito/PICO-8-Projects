--laser duel
--by oc_ram

function _init()
	cls()
	ufos = {}
	setup_ufos(ufos)
	spawn_ufo(32,32,ufos)
	spawn_ufo(96,96,ufos)
end

function _update60()
 foreach(ufos,update_ufo)
 --collisions
 for i=1,#ufos do
 	for j=i+1,#ufos do
 		collide_ufos(ufos[i], ufos[j])
 	end
 end
end

function _draw()
 cls(1)
 foreach(ufos,draw_ufo)
end

