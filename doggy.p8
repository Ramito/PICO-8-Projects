function _init()
 init_dog()
end

function _update60()
	local dir_x=0
	local dir_y=0
 if (btn(⬅️)) dir_x=-1
 if (btn(➡️)) dir_x=1
 if (btn(⬆️)) dir_y=-1
 if (btn(⬇️)) dir_y=1
 local norm=dir_x*dir_x+dir_y*dir_y
 if (norm==0) then
  dog_idle()
  return
 end
 dog_run()
 norm=1/(60*sqrt(norm))
 dir_x=dir_x*dog.speed*norm
 dir_y=dir_y*dog.speed*norm
 dog.x+=dir_x
 dog.y+=dir_y
end

function _draw()
 cls(1)
 camera()
 map()
 spr(dog.sprite,dog.x,dog.y)
end
