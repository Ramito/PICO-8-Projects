dog={}

function init_dog()
 dog = {}
 dog.speed=40
 dog.x=0
 dog.y=0
 dog.anim=0
 dog.sprite=1
end

function dog_idle()
 dog.anim=0
 dog.sprite=1
end

function dog_run()
 dog.anim+=1
 dog.anim%=20
 dog.sprite=1+dog.anim/10
end
